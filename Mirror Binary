function SymmetricTree(strArr) { 
	var split = arraySegmentor (strArr)
	for (i=1;i<split.length;i++){
		if (symmetryCheck(split[i]) === false){
			return false
		}
	}
	return true
}
function symmetryCheck (arr){
	var left = arr.slice(0,arr.length/2)
	var right = arr.slice(arr.length/2,arr.length)
	for (i=0;i<left.length;i++){
		if (left[i] !== right[right.length-1-i]){
			return false
		}
	}
	return true
}

function arraySegmentor (arr){
	var store = [], curArr = [], counter = 0
	for (i=0;i<arr.length;i++){
		var segmentLen = Math.pow(2,counter)
		curArr.push(arr[i])
		if (curArr.length === segmentLen){
			store.push(curArr)
			curArr = []
			counter ++
		}
	}
	return store
}
